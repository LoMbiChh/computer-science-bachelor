	cin >> length;

	int array[length];
	for (int i = 0; i < length; i++) {
		int x;
		cout << "Insert a number: ";
		cin >> x;
		array[i] = x;
	}

	for (int i = 1; i <= length; i++) {
		if (array[i] == array[length - 1]) {

		}
	}

	return 0;
}
