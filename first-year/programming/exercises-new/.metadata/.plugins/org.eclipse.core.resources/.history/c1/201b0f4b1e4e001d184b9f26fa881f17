}

int main() {
	int n;
	cout << "Insert a number: ";
	cin >> n;

	srand(time(0));
	int length = rand() % 10;
	cout << "length: " << length << endl;

	int array[length];
	for (int i = 0; i < length; i++) {
		array[i] = rand() % 10;
		cout << array[i] << ", ";
	}

	coutWithoutDuplicates();

	return 0;
}
