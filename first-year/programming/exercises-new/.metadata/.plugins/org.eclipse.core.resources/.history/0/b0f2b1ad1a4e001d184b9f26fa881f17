#include <iostream>
#include <cstdlib>
#include <ctime>
using namespace std;

bool isInArray(int array[], int length, int n) {
	bool isInArray = false;
	for (int i = 0; i < length; i++) if (array[i] == n) isInArray = true;

	return isInArray;
}

